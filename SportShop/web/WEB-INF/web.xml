<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>CustomerService</servlet-name>
        <servlet-class>sport.CustomerService</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>CustomerServices</servlet-name>
        <servlet-class>sport.CustomerServices</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListController</servlet-name>
        <servlet-class>sport.ListController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>sport.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustServices</servlet-name>
        <servlet-class>sport.CustServices</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCustomerController</servlet-name>
        <servlet-class>sport.AddCustomerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchCustomerController</servlet-name>
        <servlet-class>sport.SearchCustomerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchCustomerController</servlet-name>
        <servlet-class>sport.searchCustomerController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchCustomerController1</servlet-name>
        <servlet-class>sport.SearchCustomerController1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomerService</servlet-name>
        <url-pattern>/CustomerService</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerServices</servlet-name>
        <url-pattern>/CustomerServices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListController</servlet-name>
        <url-pattern>/ListController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/SearchController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustServices</servlet-name>
        <url-pattern>/CustServices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCustomerController</servlet-name>
        <url-pattern>/AddCustomerController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchCustomerController</servlet-name>
        <url-pattern>/SearchCustomerController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchCustomerController</servlet-name>
        <url-pattern>/searchCustomerController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchCustomerController1</servlet-name>
        <url-pattern>/SearchCustomerController1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.xhtml</welcome-file>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
